{
  "name": "tms-2025",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 4000",
    "dev:webpack": "next dev --no-turbo --port 4000",
    "dev:safe": "next dev --no-turbo --port 4000",
    "dev:port": "next dev --turbopack --port 4000",
    "build": "next build",
    "build:webpack": "next build --no-turbo",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "check:keys": "node scripts/check-keys.js",
    "postinstall": "prisma generate",
    "fix-team:dry": "tsx scripts/fix-request-team.ts --dry-run",
    "fix-team": "tsx scripts/fix-request-team.ts",
    "clean": "rimraf .next .turbo",
    "rebuild": "pnpm run clean && pnpm install && pnpm run build",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:seed": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "db:seed:achievements": "tsx scripts/seed-achievements.ts",
    "db:seed:templates": "tsx scripts/seed-templates.ts",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:test": "dotenv -e .env.local -- node scripts/test-db-connection.js",
    "db:setup": "dotenv -e .env.local -- prisma db push && dotenv -e .env.local -- tsx prisma/seed.ts",
    "db:reset": "dotenv -e .env.local -- prisma db push --force-reset && dotenv -e .env.local -- tsx prisma/seed.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "docker:restart": "docker-compose restart",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
